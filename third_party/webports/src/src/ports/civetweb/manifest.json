{
  "name": "Civetweb Webserver",
  "description": "Civetweb Webserver built using Native Client",
  "version": "1.5.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "storage",
    {"socket": [
      "resolve-host",
      "tcp-listen:*:*",
      "tcp-connect"
    ]}
  ]
}
