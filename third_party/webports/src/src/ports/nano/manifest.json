{
  "name": "Nano",
  "description": "Nano console application running using NativeClient",
  "version": "2.2.6.%(version)s",
  "manifest_version": 2,
  "minimum_chrome_version": "30",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "app": {
    "launch": {
      "local_path": "nano.html"
    }
  },
  "file_handlers": {
    "edit": {
      "types": [
        "text/*"
      ],
      "extensions": [
        "json",
        "txt",
        "js",
        "html",
        "h",
        "c",
        "cc",
        "cpp",
        "java"
      ],
      "title": "Edit with Nano"
    }
  },
  "permissions": [
    {"fileSystem": ["write"]},
    "clipboardRead",
    "clipboardWrite",
    "storage",
    "unlimitedStorage"
  ]
}
